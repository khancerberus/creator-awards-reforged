{
    "parser": "typescript",
    "printWidth": 100,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "plugins": ["prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.css",
            "options": {
                "parser": "css"
            }
        }
    ]
}